<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="harvests" type="harvestsType"/>

  <xs:complexType name="harvestsType">
    <xs:sequence>
      <xs:element name="harvest" type="harvestType"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="harvestType">
    <xs:sequence>
      <xs:element name="harvestid" type="xs:int"/>
      <xs:element name="fieldid" type="xs:int"/>
      <xs:element name="cropid" type="xs:int"/>
      <xs:element name="harvestdate" type="xs:date"/>
      <xs:element name="harvestedquantity" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

</xs:schema>
